package de.bachelor.smartSchedules.server.model.xmlparser.exception;

/**
 * Falls ein Event unbekannt ist.
 * @author timo
 *
 */
public class UnknownEventException extends Exception{

	/**
	 * Seriennummer
	 */
	private static final long serialVersionUID = 3813530438766526194L;

	/**
	 * Default
	 */
	public UnknownEventException() {
		
	}
	
	/**
	 * Konstruktor
	 * @param eventName
	 */
	public UnknownEventException(String eventName) {
		super(eventName +" is an unknown Event!");
	}
}
